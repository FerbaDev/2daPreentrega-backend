<h1>Lista de Productos</h1>

    {{#each products}}
<ul>
        <li> {{this.title}} </li>
        <li> {{this.description}} </li>
        <li> ${{this.price}} </li>
</ul>
<button>Agregar al carrito</button>
    {{/each}}
<br>
{{#if hasPrevPage}}
<a href="/products?page={{prevPage}}">Anerior</a>
{{/if}}
{{#if hasNextPage}}
<a href="/products?page={{nextPage}}">Siguiente</a>
{{/if}}
<p>Pagina {{currentPage}} de {{totalPages}} </p>